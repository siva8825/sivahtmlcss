function set_basic_seo_elements_safe(title,description,type,image,image_alt){description=description||title;image_alt=image_alt||description;$(document).prop('title',title);$("meta[name=description]").attr("content",description);$('meta[property="og:title"]').attr("content",title);$('meta[property="og:description"]').attr("content",description);$('meta[property="og:type"]').attr("content",type);$('meta[property="og:image"]').attr("content",image);$('meta[property="og:image:alt"]').attr("content",image_alt);$('meta[name="twitter:description"]').attr("content",description);$('meta[name="twitter:title"]').attr("content",title);}